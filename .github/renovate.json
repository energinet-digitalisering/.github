{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "dockerfile": {
        "enabled": false
    },
    "extends": [
        "config:recommended",
        ":disableDependencyDashboard"
    ],
    "flux": {
        "fileMatch": [
            "k8s/.+\\.yaml$"
        ]
    },
    "github-actions": {
        "enabled": false
    }
}